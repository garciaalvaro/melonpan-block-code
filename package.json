{
	"name": "melonpan-block-code",
	"version": "1.0.1",
	"description": "Melonpan Block Code WordPress Plugin",
	"homepage": "https://wordpress.org/plugins/melonpan-block-code/",
	"scripts": {
		"prism_themes": "webpack --mode production --config ./webpack/prism_themes.config.babel.js && rm build/_temp.js",
		"prism_lang": "webpack --mode production --config ./webpack/prism_lang.config.babel.js && rm build/_temp.js",
		"dev": "webpack --mode development --watch --config ./webpack/build.config.babel.js",
		"prod": "webpack --mode production --config ./webpack/build.config.babel.js",
		"precopy": "rm -rf _release",
		"copy": "webpack --mode production --config ./webpack/copy.config.babel.js",
		"postcopy": "rm _release/_temp.js",
		"version": "webpack --mode production --config ./webpack/version.config.babel.js",
		"postversion": "rm _temp.js",
		"prebuild": "npm run type-check && npm run version && npm run prism_lang && npm run prism_themes",
		"build": "npm run prod",
		"postbuild": "npm run copy",
		"type-check": "tsc",
		"start": "npm run dev"
	},
	"dependencies": {
		"copy-text-to-clipboard": "^2.1.1",
		"prismjs": "^1.17.1",
		"react-select": "^3.0.8"
	},
	"devDependencies": {
		"@babel/core": "^7.7.4",
		"@babel/plugin-proposal-class-properties": "^7.7.4",
		"@babel/plugin-proposal-object-rest-spread": "^7.7.4",
		"@babel/preset-env": "^7.7.4",
		"@babel/preset-react": "^7.7.4",
		"@babel/preset-typescript": "^7.7.4",
		"@babel/register": "^7.7.4",
		"@types/lodash": "^4.14.149",
		"@types/node": "^12.12.14",
		"@types/prismjs": "^1.16.0",
		"@types/react-dom": "^16.9.4",
		"@types/react-select": "^3.0.8",
		"@types/wordpress__block-editor": "^2.2.5",
		"@types/wordpress__blocks": "^6.4.4",
		"@types/wordpress__components": "^8.2.1",
		"@types/wordpress__data": "^4.6.5",
		"@types/wordpress__dom-ready": "^2.3.0",
		"@types/wordpress__element": "^2.4.0",
		"@types/wordpress__hooks": "^2.4.0",
		"@types/wordpress__html-entities": "^2.3.0",
		"@types/wordpress__i18n": "^3.4.0",
		"babel-loader": "^8.0.6",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^5.0.5",
		"css-loader": "^3.2.1",
		"file-loader": "^5.0.2",
		"mini-css-extract-plugin": "^0.8.0",
		"nib": "^1.1.2",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"string-replace-loader": "^2.2.0",
		"stylus": "^0.54.7",
		"stylus-loader": "^3.0.2",
		"typescript": "^3.7.3",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.10"
	}
}
