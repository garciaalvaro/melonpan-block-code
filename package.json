{
	"name": "melonpan-block-code",
	"version": "1.1.0",
	"description": "Melonpan Block Code WordPress Plugin",
	"homepage": "https://wordpress.org/plugins/melonpan-block-code/",
	"scripts": {
		"prism_themes": "webpack --mode production --config ./webpack/prism_themes.config.babel.js && rm build/_temp.js",
		"prism_lang": "webpack --mode production --config ./webpack/prism_lang.config.babel.js && rm build/_temp.js",
		"dev": "webpack --mode development --watch --config ./webpack/build.config.babel.js",
		"prod": "webpack --mode production --config ./webpack/build.config.babel.js",
		"precopy": "rm -rf _release",
		"copy": "webpack --mode production --config ./webpack/copy.config.babel.js",
		"postcopy": "rm _release/_temp.js",
		"version": "webpack --mode production --config ./webpack/version.config.babel.js",
		"postversion": "rm _temp.js",
		"prebuild": "npm run type-check && npm run lint && npm run version && npm run prism_lang && npm run prism_themes",
		"build": "npm run prod",
		"postbuild": "npm run copy",
		"lint": "eslint '**/*.{js,jsx,ts,tsx}'",
		"type-check": "tsc",
		"start": "npm run dev"
	},
	"dependencies": {
		"copy-text-to-clipboard": "^2.2.0",
		"prismjs": "^1.21.0",
		"react-select": "^3.1.0"
	},
	"devDependencies": {
		"@babel/core": "^7.11.1",
		"@babel/plugin-proposal-class-properties": "^7.10.4",
		"@babel/preset-env": "^7.11.0",
		"@babel/preset-react": "^7.10.4",
		"@babel/preset-typescript": "^7.10.4",
		"@babel/register": "^7.10.5",
		"@types/lodash": "^4.14.159",
		"@types/node": "^14.0.27",
		"@types/prismjs": "^1.16.1",
		"@types/react": "^16.9.45",
		"@types/react-select": "^3.0.16",
		"@types/wordpress__block-editor": "^2.2.8",
		"@types/wordpress__blocks": "^6.4.9",
		"@types/wordpress__components": "^9.0.0",
		"@types/wordpress__data": "^4.6.7",
		"@types/wordpress__dom-ready": "^2.9.0",
		"@types/wordpress__element": "^2.14.1",
		"@types/wordpress__hooks": "^2.4.0",
		"@types/wordpress__html-entities": "^2.7.0",
		"@types/wordpress__i18n": "^3.11.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"@wordpress/browserslist-config": "^2.7.0",
		"babel-loader": "^8.1.0",
		"browserslist": "^4.14.0",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^6.0.3",
		"css-loader": "^4.2.1",
		"eslint": "^7.6.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-react": "^7.20.5",
		"file-loader": "^6.0.0",
		"mini-css-extract-plugin": "^0.9.0",
		"nib": "^1.1.2",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"prettier": "^2.0.5",
		"string-replace-loader": "^2.3.0",
		"stylus": "^0.54.8",
		"stylus-loader": "^3.0.2",
		"typescript": "^3.9.7",
		"webpack": "^4.44.1",
		"webpack-cli": "^3.3.12"
	}
}
